<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="systemRoleMapper">
	<!--表名 -->
	<sql id="tableName">
		"TBRD_YH_CURING_DATA"
	</sql>

	<!-- 字段 -->
	<sql id="Field">
		"FILE_NAME",
		"CURING_DATA_TYPE",
		"UPLOAD_DATE",
		"UPLOAD_PERSON",
		"EFFECTIVE",
		"PICTURE",
		"REMARK",
		"CREATE_USER",
		"CREATE_DATE",
		"UPDATE_USER",
		"UPDATE_DATE",
		"IS_DEL",
		"ID"
	</sql>

	<!-- 字段值 -->
	<sql id="FieldValue">
			#{FILE_NAME},
			#{CURING_DATA_TYPE},
			#{UPLOAD_DATE},
			#{UPLOAD_PERSON},
			#{EFFECTIVE},
			#{PICTURE},
			#{REMARK},
			#{CREATE_USER},
			#{CREATE_DATE},
			#{UPDATE_USER},
			#{UPDATE_DATE},
			#{IS_DEL},
			#{ID}
	</sql>

	<!-- 新增-->
	<insert id="save" parameterType="pd">
		insert into
		<include refid="tableName"></include>
		(
		<include refid="Field"></include>
		) values (
		<include refid="FieldValue"></include>
		)
	</insert>
</mapper>
